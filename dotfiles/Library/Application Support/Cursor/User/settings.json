{
	"biome.searchInPath": false,
	"editor.defaultFormatter": "biomejs.biome",
	"editor.formatOnSave": true,
	"editor.semanticHighlighting.enabled": true,
	"editor.matchBrackets": "never",
	"extensions.experimental.affinity": {
		"asvetliakov.vscode-neovim": 1
	},
	"editor.bracketPairColorization.enabled": false,
	"security.promptForLocalFileProtocolHandling": false,
	"security.workspace.trust.enabled": false,
	"security.workspace.trust.untrustedFiles": "open",
	"workbench.editorAssociations": {
		"*.plist": "default"
	},
	"cursor.cpp.disabledLanguages": ["plaintext", "markdown", "scminput"],

	"apc.electron": {
		"frame": false
	},

	"workbench.colorTheme": "Ayu Mirage",
	"breadcrumbs.enabled": false,
	"workbench.colorCustomizations": {
		// Borders
		"foreground": "#d9d7ce",
		"sideBar.border": "#11141b",
		"editorGroupHeader.tabsBorder": "#11141b",
		"editorGroup.border": "#11141b",
		"titleBar.border": "#11141b",
		"tab.border": "#11141b",
		"statusBar.border": "#11141b",
		"sideBySideEditor.verticalBorder": "#11141b",
		"sideBySideEditor.horizontalBorder": "#11141b",
		"editor.compositionBorder": "#11141b",

		// Backgrounds
		"editor.background": "#1A1F28",
		"sideBar.background": "#1A1F28",
		"tab.activeBackground": "#1A1F28",
		"tab.inactiveBackground": "#1A1F28",
		"titleBar.activeBackground": "#1A1F28",
		"editorGroupHeader.tabsBackground": "#1A1F28",
		"editor.border": "#1A1F28",
		"sideBarSectionHeader.background": "#1A1F28",
		"sideBarSectionHeader.border": "#1A1F28",

		"focusBorder": "#686868",
		"statusBar.background": "#1A1F28",
		"statusBar.foreground": "#d9d7ce",

		"gitDecoration.addedResourceForeground": "#d9d7ce",
		"gitDecoration.modifiedResourceForeground": "#d9d7ce",
		"gitDecoration.deletedResourceForeground": "#d9d7ce",
		"gitDecoration.renamedResourceForeground": "#d9d7ce",
		"gitDecoration.stageModifiedResourceForeground": "#d9d7ce",
		"gitDecoration.stageDeletedResourceForeground": "#d9d7ce",
		"gitDecoration.untrackedResourceForeground": "#d9d7ce",
		"gitDecoration.ignoredResourceForeground": "#d9d7ce",
		"gitDecoration.conflictingResourceForeground": "#d9d7ce",
		"gitDecoration.submoduleResourceForeground": "#d9d7ce"
	},
	"editor.tokenColorCustomizations": {
		"textMateRules": [
			// comments
			{
				"scope": "entity.name.type.instance.jsdoc",
				"settings": { "foreground": "#B8CFE680" }
			},
			{
				"scope": "storage.type.class.jsdoc",
				"settings": { "foreground": "#B8CFE680" }
			},
			{
				"scope": "variable.other.jsdoc",
				"settings": { "foreground": "#B8CFE680" }
			},
			//reset
			{ "scope": "constant.other", "settings": { "foreground": "#d9d7ce" } },
			{ "scope": "entity.name", "settings": { "foreground": "#d9d7ce" } },
			{
				"scope": "entity.name.function",
				"settings": { "foreground": "#d9d7ce" }
			},
			{ "scope": "keyword.operator", "settings": { "foreground": "#d9d7ce" } },
			{ "scope": "meta.parameter", "settings": { "foreground": "#d9d7ce" } },
			{
				"scope": "punctuation.accessor",
				"settings": { "foreground": "#d9d7ce" }
			},
			{
				"scope": "punctuation.separator",
				"settings": { "foreground": "#d9d7ce" }
			},
			{
				"scope": "string.unquoted.argument.shell",
				"settings": { "foreground": "#d9d7ce" }
			},
			{
				"scope": "variable.parameter",
				"settings": { "foreground": "#d9d7ce" }
			},

			// Yellow
			{ "scope": "entity.name.tag", "settings": { "foreground": "#ffdf80" } },
			{ "scope": "meta.type", "settings": { "foreground": "#ffdf80" } },
			{
				"scope": "meta.type.annotation",
				"settings": { "foreground": "#ffdf80" }
			},
			{
				"scope": "meta.type.declaration",
				"settings": { "foreground": "#ffdf80" }
			},
			{ "scope": "meta.type.tuple", "settings": { "foreground": "#ffdf80" } },
			{
				"scope": "punctuation.definition.tag.begin",
				"settings": { "foreground": "#ffdf80" }
			},
			{
				"scope": "punctuation.definition.tag.end",
				"settings": { "foreground": "#ffdf80" }
			},
			{
				"scope": "punctuation.definition.typeparameters",
				"settings": { "foreground": "#ffdf80" }
			},
			{
				"scope": "punctuation.section.embedded.begin",
				"settings": { "foreground": "#ffdf80" }
			},
			{
				"scope": "punctuation.section.embedded.end",
				"settings": { "foreground": "#ffdf80" }
			},
			{ "scope": "storage.type.type", "settings": { "foreground": "#ffdf80" } },
			{
				"scope": "support.class.component",
				"settings": { "foreground": "#ffdf80" }
			},
			{ "scope": "support.type", "settings": { "foreground": "#ffdf80" } },
			{
				"scope": "support.type.property-name",
				"settings": { "foreground": "#ffdf80" }
			},

			// Green
			{ "scope": "string", "settings": { "foreground": "#cbe645" } },
			{
				"scope": "punctuation.definition.template-expression",
				"settings": { "foreground": "#cbe645" }
			},

			// Blue
			{ "scope": "constant.language", "settings": { "foreground": "#6871ff" } },
			{ "scope": "constant.numeric", "settings": { "foreground": "#6871ff" } },

			// Keywords
			{ "scope": "keyword", "settings": { "foreground": "#f07178" } },
			{ "scope": "keyword.control", "settings": { "foreground": "#f07178" } },
			{ "scope": "storage", "settings": { "foreground": "#f07178" } },
			{ "scope": "storage.type", "settings": { "foreground": "#f07178" } },
			{
				"scope": "storage.type.function",
				"settings": { "foreground": "#f07178" }
			},
			{ "scope": "support.function", "settings": { "foreground": "#f07178" } },
			{ "scope": "variable.language", "settings": { "foreground": "#f07178" } }
		]
	},
	"editor.semanticTokenColorCustomizations": {
		"rules": {
			"parameter": "#d9d7ce",
			"function": "#d9d7ce",
			"namespace": "#d9d7ce",
			"method": "#d9d7ce",
			"class": "#d9d7ce",

			"type": "#ffdf80",
			"interface": "#ffdf80",
			"enum": "#ffdf80"
		}
	},
	"workbench.editor.editorActionsLocation": "titleBar",
	"window.customTitleBarVisibility": "windowed",
	"window.commandCenter": true,
	"workbench.editor.showTabs": "none",
	"cursor.composer.shouldChimeAfterChatFinishes": true,
	"accessibility.signals.taskCompleted": {
		"sound": "on",
		"announcement": "auto"
	},
	"accessibility.signals.taskFailed": {
		"sound": "on",
		"announcement": "auto"
	},
	"accessibility.signals.terminalBell": {
		"sound": "on",
		"announcement": "auto"
	},
	"accessibility.signals.terminalCommandFailed": {
		"sound": "on",
		"announcement": "auto"
	},
	"accessibility.signals.terminalCommandSucceeded": {
		"sound": "on",
		"announcement": "auto"
	},
	"accessibility.signals.terminalQuickFix": {
		"sound": "on",
		"announcement": "auto"
	},
	"accessibility.signals.voiceRecordingStarted": {
		"sound": "auto"
	},
	"biome.suggestInstallingGlobally": false,
	"editor.codeActionsOnSave": {
		"source.biome": "always"
		
	},
}

